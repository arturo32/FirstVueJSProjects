treturn{\n\t\t\t\tcurrencyList:[],\n\t\t\t\tstyleList: {height: '29px'},\n\t\t\t\tcurrencySelected: 'BRL',\n\t\t\t\tgeneralCurrency_value: ''\n\t\t\t}\n\t\t},\n\t\tmethods:{\n\t\t\tchangeList(){\n\t\t\t\tlet url = 'https://api.exchangeratesapi.io/latest';\n\t\t\t\tfetch(url).then(res => res.json()).then(json => { \n\t\t\t\t\tthis.currencyList = Object.getOwnPropertyNames(json.rates);\n\t\t\t\t});\n\n\t\t\t\tvar arrow = document.querySelector(\".arrow\");\n\t\t\t\tif(this.styleList.height == \"29px\"){\n\t\t\t\t\tthis.styleList.height = \"493px\";\n\t\t\t\t\tarrow.style.transform = \"rotate(-135deg)\";\n\t\t\t\t\tarrow.style.top = \"10px\";\n\t\t\t\t}\n\t\t\t\telse{\n\t\t\t\t\tthis.styleList.height = \"29px\";\n\t\t\t\t\tarrow.style.transform = \"rotate(45deg)\";\n\t\t\t\t\tarrow.style.top = \"7px\";\n\t\t\t\t}\n\t\t\t\t\n\t\t\t},\n\t\t\tselectCurrency(c){\n\t\t\t\tthis.currencySelected = c;\n\n\t\t\t}\n\t\t},\n\t\tcomputed:{\n\t\t\tcurrencies(){\n\t\t\t\treturn this.currencyList;\n\t\t\t}\n\t\t}\n\t}\n\n\n\n\n</script>\n\n\n<style scoped>\n\t.options{\n\t\tmargin-bottom: 100px;\n\t}\n\th2{\n\t\ttext-align:center;\n\t\tmargin: 0;\n\t}\n\n\t#lang_options{\n\t\twidth: 85px;\n\t\tborder: 0px;\n\t\tdisplay: inline-block; \n\t\tmargin-top: 40px;\n\t\tbackground-color: white;\n\t\tcolor: var(--titlesColor);\n\t\tborder-radius: 20px;\n\t\tposition: absolute;\n\t\tfont-family: Arial, sans-serif;\n\t\tfont-weight: 600;\n\t\tfont-size: 16px;\n\t\tcursor: pointer;\n\t\ttransition: all 300ms ease-out;\n\t\tbox-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2);\n\t\theight: 29px;   \n\t\toverflow-y: hidden; \n\t\tleft: 0;\n\t\tright: 0;\n\t\tmargin-left: auto;\n\t\tmargin-right: auto;\n\t}\n\n\t#lang_options p{\n\t\tmargin: 0px;\n\t\tpadding-top: 5px;\n\t\tpadding-bottom: 5px;\n\t\tpadding-right: 10px;\n\t}\n\n\n\t.lang_item:hover{\n\t\tbackground-color: #d9d9d9;\n\t}\n\n\t.arrow {\n\t\tborder: solid black;\n\t\tborder-width: 0 3px 3px 0;\n\t\tdisplay: inline-block;\n\t\tpadding: 3px;\n\t\t